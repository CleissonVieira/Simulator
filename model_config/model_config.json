            "origem": "componente_gerador",
            "destino": ["componente_roteador","roteadorFilaCaixa"]
        },
        "filaMedico": {
            "origem": "atendCaixa",
            "destino": ["componente_finito", "atendMedico"]
        }
    },

    "componente_finito": {
        "atendCaixa": {
            "origem": "filaCaixa",
            "destino": ["componente_fila", "filaMedico"],
            "n_atendentes": 2,
            "intervalo_gasto": [2,7]
        },
        "atendMedico": {
            "origem": "filaMedico",
            "destino": ["componente_saida", "saida"],
            "n_atendentes": 3,
            "intervalo_gasto": [7,15]
        }
    },

    "componente_infinito": {
    },

    "componente_saida": {
        "saida": {
            "origem": "atendCaixa"
        }
    }
}