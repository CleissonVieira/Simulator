{
    "tempo_simulacao": 80,

    "componente_gerador": {
        "destino": ["componente_fila","filaCaixa"],
        "limite": 1000,
        "intervalo_tempo_de_operação": [3,21],
        "intervalo_entre_geracao": 2
    },

    "componente_roteador": {
        "roteadorFilaCaixa": {
            "origem": "atendCaixa",
            "destinos": [["fila","filaMedico", 80], ["componente_saida", "saida", 20]]
        }
    },

    "componente_fila": {
